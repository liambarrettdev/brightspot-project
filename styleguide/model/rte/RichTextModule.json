{
    "_template": "/model/rte/RichTextModule",
    "body": {
        "_template": "base/util/Concatenated",
        "items": [
            {
                "_template": "/base/util/RawHtml",
                "html": "{{paragraphs(2)}}"
            },
            {
                "_template": "/base/Enhancement",
                "alignment": "left",
                "content": {
                    "_dataUrl": "/model/image/Image.json"
                }
            },
            {
                "_template": "/base/Enhancement",
                "alignment": "left",
                "content": {
                    "_dataUrl": "/model/video/Video.json"
                }
            },
            {
                "_template": "/base/Enhancement",
                "alignment": "left",
                "content": {
                    "_dataUrl": "/model/audio/Audio.json"
                }
            },
            {
                "_template": "/base/Enhancement",
                "alignment": "left",
                "content": {
                    "_template": "/model/quote/QuoteModule",
                    "quote": "{{words(20)}}",
                    "image": {
                        "_template": "/base/util/Image",
                        "src": "https://static.vecteezy.com/system/resources/previews/014/194/216/non_2x/avatar-icon-human-a-person-s-badge-social-media-profile-symbol-the-symbol-of-a-person-vector.jpg"
                    }
                }
            },
            {
                "_template": "/base/util/RawHtml",
                "html": "{{paragraphs(1)}}"
            }
        ]
    }
}
