{
    "_wrapper": false,
    "_template": "/model/page/Page",
    "locale": "en",
    "head": {
        "_dataUrl": "/model/page/Head.json"
    },
    "breadcrumbs": [
        {
            "_dataUrl": "/model/link/Link.json",
            "_repeat": 2
        },
        {
            "_template": "/base/util/Link",
            "body": "Current Page"
        }
    ],
    "header" : {
        "_dataUrl": "/model/page/header/Header.json"
    },
    "above": {
        "_dataUrl": "/model/page/module/Above.json"
    },
    "main": {
        "_template": "base/util/Concatenated",
        "items": [
            {
                "_template": "base/util/Concatenated",
                "items": [
                    {
                        "_template": "/base/util/RawHtml",
                        "html": "{{paragraphs([3, 10])}}"
                    }
                ]
            }
        ]
    },
    "aside": {
        "_dataUrl": "/model/page/module/Aside.json"
    },
    "below": {
        "_dataUrl": "/model/page/module/Below.json"
    },
    "footer": {
        "_dataUrl": "/model/page/footer/Footer.json"
    }
}
